<% 
	Cookie[] cookies = request.getCookies();

	StringBuilder sb = new StringBuilder();
	for(Cookie c : cookies){
		String cName = c.getName();
		String cValue = c.getValue();
		sb.append("쿠키이름 : " + cName + ", 쿠키 값 : " + cValue)
	}
%>
